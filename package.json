{
  "name": "una-platform-lite",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"vite\" \"node server.cjs\"",
    "dev:frontend": "vite",
    "dev:server": "node server.cjs",
    "dev:clean": "./scripts/start-clean.sh",
    "dev:kill": "lsof -ti :5173 :5174 :3000 :3001 | xargs kill -9 2>/dev/null || true",
    "dev:check": "lsof -i :5173 -i :5174 -i :3000 -i :3001",
    "setup:env": "./scripts/setup-env.sh",
    "setup:webhooks": "./scripts/setup-stripe-webhooks.sh",
    "seed": "ts-node scripts/seed.ts",
    "stripe:test": "stripe trigger checkout.session.completed",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.1",
    "@mdx-js/loader": "^3.1.0",
    "@mdx-js/react": "^3.1.0",
    "@stripe/stripe-js": "^7.9.0",
    "@supabase/supabase-js": "^2.57.4",
    "@vercel/speed-insights": "^1.2.0",
    "docx": "^9.5.1",
    "gray-matter": "^4.0.3",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.2",
    "lucide-react": "^0.400.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.62.0",
    "react-router-dom": "^7.8.2",
    "stripe": "^18.5.0",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@types/jspdf": "^1.3.3",
    "@types/node": "^24.4.0",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.0",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.1",
    "cypress": "^15.2.0",
    "dotenv": "^17.2.2",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "express": "^5.1.0",
    "globals": "^16.3.0",
    "http-proxy-middleware": "^3.0.5",
    "postcss": "^8.5.6",
    "puppeteer": "^24.22.0",
    "tailwindcss": "^3.4.17",
    "terser": "^5.44.0",
    "ts-node": "^10.9.2",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.1",
    "vite": "^7.1.2"
  }
