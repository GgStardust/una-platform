{
  "builds": [
    {
      "src": "una-platform/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "una-platform/api/$1.js"
    },
    {
      "src": "/(.*)",
      "dest": "una-platform/dist/$1"
    }
  ]
}
