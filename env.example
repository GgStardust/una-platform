# Site Configuration
VITE_SITE_URL=http://localhost:5173
VITE_SITE_NAME=UNA Formation Platform
# Server-side site URL (for API routes)
SITE_URL=http://localhost:5173

# Admin Configuration
VITE_ADMIN_ALLOWED_EMAILS=admin@example.com
VITE_ADMIN_PASSWORD=your_admin_password_here

# Stripe Configuration
# Frontend Stripe publishable key (safe to expose in client-side code)
VITE_STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_publishable_key_here
# Backend Stripe secret key (keep secure, server-side only)
STRIPE_SECRET_KEY=sk_test_your_stripe_secret_key_here
# Stripe webhook endpoint secret for verifying webhook signatures
STRIPE_WEBHOOK_SECRET=whsec_your_webhook_secret_here

# Stripe Product Price IDs
# Price ID for $250 Strategy Session
STRIPE_STRATEGY_SESSION_PRICE_ID=price_your_strategy_session_price_id_here
# Price ID for $750 Document Preparation
STRIPE_DOCUMENT_PREP_PRICE_ID=price_your_document_prep_price_id_here
# Price ID for $1000 Bundle (Strategy Session + Document Prep)
STRIPE_BUNDLE_PRICE_ID=price_your_bundle_price_id_here

# Supabase Configuration
# Supabase project URL
VITE_SUPABASE_URL=https://your-project.supabase.co
# Supabase anonymous key (safe to expose in client-side code)
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here